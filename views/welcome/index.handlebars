<header class="page-header text-center">
  <h1>User Registration</h1>
</header>


{{#if result }}
  <pre>{{ json result }}</pre>
{{/if }}


<form action="/emails" method="post">

  <div class="form-group">
    <label for="email_options_to">Email</label>
    <input type="text" id="email_options_to" name="email_options[to]" class="form-control">
  </div>

  <div class="form-group">
    <label for="password">Password</label>
    <input type="password" id="password" name="password" class="form-control">
  </div>

  <div class="form-group">
    <label for="email_options_fname">First Name</label>
    <input type="text" id="email_options_fname" name="email_options[fname]" class="form-control">
  </div>

  <div class="form-group">
    <label for="email_options_lname">Last Name</label>
    <input type="text" id="email_options_lname" name="email_options[lname]" class="form-control">
  </div>

  <div class="form-group">
    <input type="submit" class="btn btn-success">
  </div>
</form>
